<Window x:Class="FIFA22_INFO.AllTeam"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FIFA22_INFO"
        mc:Ignorable="d"
        WindowStyle="None"
        AllowsTransparency="True"
        BorderBrush="Black"
        BorderThickness="1"
        Title="AllTeam" Height="910" Width="545"
        KeyDown="Exit_KeyDown">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="50"/>
            <RowDefinition Height="40"/>
            <RowDefinition Height="820"/>
        </Grid.RowDefinitions>
        <Grid Grid.Row="0">
            <Rectangle MouseLeftButtonDown="Rectangle_MouseDown">
                <Rectangle.Fill>
                    <SolidColorBrush Color="#4cac35"/>
                </Rectangle.Fill>
            </Rectangle>
            <DockPanel HorizontalAlignment="Left" Width="250">
                <Image Source="Resources/FIFA22.png" Stretch="Fill" Width="220" Height="47"/>
            </DockPanel>
        </Grid>
        <Grid Grid.Row="1">
            <Rectangle>
                <Rectangle.Fill>
                    <SolidColorBrush Color="LightGray"/>
                </Rectangle.Fill>
            </Rectangle>
            <Label x:Name="label2_Copy" Content="TEAM : " HorizontalAlignment="Left" VerticalAlignment="Top" FontSize="13" Foreground="Blue" BorderThickness="1" Margin="-1,0,0,0"/>
            <TextBox x:Name="ProdCode_Textbox" 
                     HorizontalAlignment="Left" 
                     Margin="57,6,0,0" 
                     TextWrapping="Wrap" 
                     VerticalAlignment="Top" 
                     Width="93" 
                     Height="20" 
                     MaxLength="6"
                     TextChanged="Upper_TextChanged"
                     />
            <Button x:Name="Select_Button" Content="검 색" HorizontalAlignment="Left" Margin="155,4,0,0" VerticalAlignment="Top" Width="60" BorderBrush="Green" BorderThickness="3" Height="24" FontSize="12" />
            <Button x:Name="Pick_Button" Content="선 택" HorizontalAlignment="Left" Margin="215,4,0,0" VerticalAlignment="Top" Width="60" BorderBrush="Green" BorderThickness="3" Height="24" FontSize="12" />
            <Button x:Name="Cancel_Button" Content="취 소" HorizontalAlignment="Left" Margin="275,4,0,0" VerticalAlignment="Top" Width="60" BorderBrush="Green" BorderThickness="3" Height="24" FontSize="12" Click="Cancel_Click"/>
        </Grid>
