<Window x:Class="FIFA22_INFO.TeamCareer"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FIFA22_INFO"
        mc:Ignorable="d"
        WindowStyle="None"
        ResizeMode="NoResize"
        Title="TeamCareer" Height="500" Width="1100"
        KeyDown="KeyEvent">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="50"/>
            <RowDefinition Height="40"/>
            <RowDefinition Height="410"/>
        </Grid.RowDefinitions>

        <Grid Grid.Row="0">
            <Rectangle MouseLeftButtonDown="Rectangle_MouseDown">
                <Rectangle.Fill>
                    <SolidColorBrush Color="#4cac35"/>
                </Rectangle.Fill>
            </Rectangle>
            <DockPanel HorizontalAlignment="Left" Width="250">
                <Image Source="Resources/FIFA22.png" Stretch="Fill" Width="220" Height="47"/>
            </DockPanel>
            <DockPanel HorizontalAlignment="Right">
                <Button x:Name="ToMiniButton" Margin="0,0,5,0" 
                        Click="ToMiniButton_Click"
                        Width="25"
                        Height="25"
                        Background="#4cac35"
                        BorderThickness="0">
                    <Image Source="Resources/Minimization.png" Stretch="fill"/>
                </Button>
                <Button x:Name="CloseButton" Margin="0,0,30,0"
                        Click="CloseButton_Click"
                        Width="25"
                        Height="25"
                        Background="#4cac35"
                        BorderThickness="0">
                    <Image Source="Resources/Close.png" Stretch="fill"/>
                </Button>
            </DockPanel>
        </Grid>

        <Grid Grid.Row="1">
            <Button x:Name="CHAMPIONS_button" Content="CHAMPIONS" HorizontalAlignment="Left" Grid.Row="1" VerticalAlignment="Center" Height="40" Width="102" Click="champions_Click"/>
            <Button x:Name="RUNNERUP_button" Content="RUNNER-UP" HorizontalAlignment="Left" Grid.Row="1" VerticalAlignment="Center" Height="40" Width="106" Margin="107,0,0,0" Click="Runnerup_Click"/>
            <TextBox x:Name="Option_textBox" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" IsReadOnly="True" IsHitTestVisible="False" HorizontalAlignment="Left" Margin="244,0,0,0" TextWrapping="Wrap"  VerticalAlignment="Center" Width="828" BorderThickness="0" FontWeight="Bold" FontSize="20">
                <TextBox.Style>
                    <Style TargetType="TextBox">
                        <Style.Triggers>
                            <DataTrigger Binding="{Binding Path = OptionName}" Value="CHAMPIONS_LEAGUE">
                                <Setter Property="Foreground" Value="#04048c"/>
                            </DataTrigger>
                        </Style.Triggers>
                    </Style>
                </TextBox.Style>
            </TextBox>

        </Grid>

        <Grid Grid.Row="2">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="250"/>
                <ColumnDefinition Width="850"/>
            </Grid.ColumnDefinitions>

            <Grid Grid.Column="0">
                <Rectangle x:Name="imageRec" HorizontalAlignment="Left" Height="187" Margin="17,70,0,0" Stroke="Black" VerticalAlignment="Top" Width="180" StrokeThickness="0"
                           />
